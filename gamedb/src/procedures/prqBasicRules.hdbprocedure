PROCEDURE "novogame.gamedb.procedures::prqBasicRules"
(
	out rsBasicRules "novogame.gamedb.views::cvqBasicRules"
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   AS
BEGIN
	
	rsBasicRules = 
		SELECT 
			"id",
			"cluster",
			"buyer",
			"indicator",
			"initialDate",
			"finalDate",
			"user",
			"creationDate",
			"type",
			"status"
		FROM "novogame.gamedb.views::cvqBasicRules" as basicRules;

END