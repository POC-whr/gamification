PROCEDURE "novogame.gamedb.procedures::prcSku"
(
	IN paramin "novogame.gamedb.tables::sku.sku"
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
  -- DEFAULT SCHEMA "GAMIFICATION_HDI_GAMEDB_2"
   AS
BEGIN

	UPSERT "novogame.gamedb.tables::sku.sku"
	(
		"id",       
		"productId", 
		"isActive"
	)
	SELECT
		"id",       
		"productId", 
		"isActive"
	FROM :paramin;

END