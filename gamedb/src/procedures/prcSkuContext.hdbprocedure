PROCEDURE "novogame.gamedb.procedures::prcSkuContext"
( 
	IN paramin "novogame.gamedb.tables::skuContext.skuContext"
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   AS
BEGIN

	UPSERT "novogame.gamedb.tables::skuContext.skuContext"
	(
		"id",           
		"productId",   
		"skuName",    
		"isActive"    		
	)
	SELECT
		"id",           
		"productId",   
		"skuName",    
		"isActive"  
	FROM :paramin;
	
END