PROCEDURE "novogame.gamedb.procedures::prcRegraCategoria"
(
	IN paramin "novogame.gamedb.tables::regraCategoria.regraCategoria"
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   AS
BEGIN

	UPSERT "novogame.gamedb.tables::regraCategoria.regraCategoria"
	(
		"idCategoria",
		"idRegra",
		"categoria",
		"status"
	)
	SELECT
		"idCategoria",
		"idRegra",
		"categoria",
		"status"
	FROM :paramin;

END