   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   AS
BEGIN

	UPSERT "novogame.gamedb.tables::categoryTree.categoryTree"
	(
		"id",
		"name"
	)
	SELECT
		"id",
		"name"
	FROM :paramin; 

END