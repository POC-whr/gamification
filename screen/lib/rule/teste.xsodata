service {

//"apis.headers" as "headers"
"apis.headers" as "headers" navigates ("ToApisAssctn" as "apis")
create events(before "rule.xsjs:teste.xsjslib::header_create_before_exit");

"apis.apis"   as "apis"
create events(before "rule.xsjs:teste.xsjslib::apis_create_before_exit");

	//AssociaÃ§Ãµes
association "ToApisAssctn" principal "headers"("id") 
