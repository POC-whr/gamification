using types;
context productRule {

	entity productRule {
	key	id          : types.id;
		idSaleType  : types.idSaleType;
		name        : types.name;
		version     : types.version;
		validityIni : types.validityIni;
		validityEnd : types.validityEnd;
		validtyDays : types.validtyDays;
		status      : types.status;
	}
};