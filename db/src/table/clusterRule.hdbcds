using types;
context clusterRule {

	entity clusterRule {
		key	id             : types.id;
			idCluster      : types.idCluster;
			typeBuyer      : types.typeBuyer;
			buyerValue     : types.buyerValue;
			recurrence     : types.recurrence;
			target         : types.target;
			typeIndicated  : types.typeIndicated;
			indicatedValue : types.indicatedValue;
			status         : types.status;
			days           : types.days;
			audit		   : types.Audit;
	}
};